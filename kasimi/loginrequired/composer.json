{
    "name": "kasimi/loginrequired",
    "type": "phpbb-extension",
    "description": "Forcefully redirects any non-logged in user to the login page with the possibility to add exceptions to a whitelist.",
    "homepage": "https://github.com/kasimi",
    "version": "1.0.3",
    "time": "2015-18-01",
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "kasimi",
            "email": "",
            "homepage": "https://www.phpbb.com/community/memberlist.php?mode=viewprofile&u=1330603",
            "role": "Lead Developer"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "phpbb/phpbb": ">=3.1.0@dev"
    },
    "extra": {
        "display-name": "Login Required",
        "soft-require": {
            "phpbb/phpbb": ">=3.1.0@dev"
        },
        "version-check": {
            "host": "www.phpbb.com",
            "directory": "/customise/db/extension/login_required",
            "filename": "version_check"
        }
    }
}